1	Solid Rectangular Star Pattern in C
#include <stdio.h>

int main()
{

int rows, columns,i,j;

printf("Enter the number of rows : ");
scanf("%d", &rows);

printf("Enter the number of columns : ");
scanf("%d", &columns);

printf("\n");

/* print solid rectangle*/

for (i = 1; i <= rows; i++)
{
      for (j = 1; j <= columns; j++)
       {
             printf("*");
       }
      printf("\n");
}
return 0;
}

2. Hollow Rectangular Star Pattern in C
#include <stdio.h>
int main()
{
int rows, columns,i, j;

printf("Enter the number of rows : ");
scanf("%d", &rows);
printf("Enter the number of columns : ");
scanf("%d", &columns);

printf("\n");
for (i = 1; i <= rows; i++)

{
	for (j = 1; j <= columns; j++)
	{	
    	if (i==1 || i==rows || j==1 || j==columns)
			printf("*");
		else
			printf(" ");
	}
	printf("\n");
}
return 0;
}
