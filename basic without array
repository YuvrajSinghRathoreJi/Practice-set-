  scanf("%d",&width);  
 for( i=1;i<=width;i++)  
 {  
   for( j=1;j<=width-i;j++)  
   {  
       printf(" ");  
   }  
   for( k=0;k<=width-i;k++)  
   {  
     printf("*");  
   }  
   printf("\n");  
 }  
