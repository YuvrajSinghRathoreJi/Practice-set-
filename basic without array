    printf("Enter a natural number n: ");
    scanf("%d", &n);
    if (n < 1) {
        printf("Please enter a positive natural number.\n");
        return 1;
    }
    while (n >= 1) {
        printf("%d ", n);
        n--;
    }

    printf("\n");

    return 0;
}
