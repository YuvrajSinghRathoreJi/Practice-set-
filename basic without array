  return 0;  
}
13. Plus Star Pattern in C
#include<stdio.h>  
  
int main() {  
  int height,i,j;  
  printf("Enter the height (odd): ");  
  scanf("%d", &height);  
  for( i=1;i<=height;i++)  
  {  
    if(i==((height/2)+1))  
    {  
      for( j=1;j<=height;j++)  
      {  
        printf("+");  
      }  
   
    }  
    else  
    {  
    for( j=1;j<=height/2;j++)  
    {  
      printf(" ");  
    }  
    printf("+");  
    }  
    printf("\n");  
  }  
  return 0;  
}
14. X Pattern in C
#include<stdio.h> 
  
int main() {  
  int width,space, i,j;  
  printf("Enter the width: ");  
  scanf("%d",&width);  
  space=2*width-1;  
  for( i=1;i<=space;i++)  
  {  
    for( j=1;j<=space;j++)  
    {  
       if(i==j || j==(space-i+1))  
       {  
         printf("*");  
       }  
       else  
       {  
         printf(" ");  
       }  
    }  
    printf("\n");  
  }  
  return 0;  
}
